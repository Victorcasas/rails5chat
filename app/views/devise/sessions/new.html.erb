<div class="col-lg-5">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email, autofocus: false, :input_html => { :value => 'invitado@mail.com' }, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password, autocomplete: "off", :input_html => { :value => '111111' }, class: 'form-control' %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="form-group">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

    <%= f.submit "Acceder", class: 'btn btn-primary' %>
<% end %>

<%= render "devise/shared/links" %>
</div>
